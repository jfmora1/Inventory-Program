<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller_ModifyPart">
    <children>
        <HBox layoutX="77.0" layoutY="38.0" prefHeight="52.0" prefWidth="463.0">
