      <TextField fx:id="AddMacID_part" layoutX="194.0" layoutY="415.0" />
      <TextField fx:id="AddMin_part" layoutX="437.0" layoutY="352.0" prefHeight="25.0" prefWidth="127.0" />
      <Label layoutX="373.0" layoutY="356.0" text="Min" />
