      </HBox>
      <Label layoutX="92.0" layoutY="120.0" text="ID" />
      <Label layoutX="92.0" layoutY="177.0" text="Name" />
      <Label layoutX="92.0" layoutY="233.0" text="Inventory" />
      <Label layoutX="92.0" layoutY="292.0" text="Price/Cost" />
      <Label layoutX="92.0" layoutY="356.0" text="Max" />
      <Label fx:id="MachineID" layoutX="92.0" layoutY="419.0" text="Machine ID" />
      <TextField fx:id="AddID_part" editable="false" layoutX="194.0" layoutY="116.0" promptText="Auto-Gen Disabled">
